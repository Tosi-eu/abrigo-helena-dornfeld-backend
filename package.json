{
  "name": "estoque-armarios",
  "version": "1.0.0",
  "main": "dist/server.js",
  "license": "MIT",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/infrastructure/web/main.ts",
    "dev:server": "ts-node-dev --respawn --transpile-only src/infrastructure/database/connection.ts",
    "build": "tsc",
    "start": "node dist/infrastructure/web/main.js",
    "start:local": "NODE_ENV=development npm run dev",
    "test": "jest tests/unit",
    "test:e2e": "jest tests/e2e --detectOpenHandles",
    "test:watch": "jest --watchAll",
    "format": "prettier --write \"src/**/*.{ts,js,json}\"",
    "lint": "eslint \"src/**/*.ts\" --fix",
    "db:migrate": "sequelize-cli db:migrate",
    "db:migrate:undo": "sequelize-cli db:migrate:undo",
    "db:seed": "sequelize-cli db:seed:all"
  },
  "dependencies": {
    "@types/pdfkit": "^0.17.3",
    "axios": "^1.13.2",
    "bcrypt": "^6.0.0",
    "cheerio": "^1.1.2",
    "cookie-parser": "^1.4.6",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.5.1",
    "helmet": "^7.2.0",
    "ioredis": "^5.8.2",
    "jsonwebtoken": "^9.0.3",
    "node-cron": "^4.2.1",
    "pdfkit": "^0.17.2",
    "pg": "^8.11.3",
    "sequelize": "^6.37.7",
    "supertest": "^7.1.4",
    "winston": "^3.19.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@types/bcrypt": "^6.0.0",
    "@types/cheerio": "^0.22.35",
    "@types/cookie-parser": "^1.4.7",
    "@types/express": "^4.17.25",
    "@types/express-serve-static-core": "^5.1.0",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^20.19.25",
    "@types/node-cron": "^3.0.11",
    "@types/sequelize": "^4.28.20",
    "@types/supertest": "^6.0.3",
    "@types/winston": "^2.4.4",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "babel-jest": "^30.2.0",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^10.1.8",
    "globals": "^16.5.0",
    "jest": "^30.2.0",
    "jiti": "^2.6.1",
    "prettier": "^3.7.4",
    "sequelize-cli": "^6.6.2",
    "ts-jest": "^29.4.5",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.48.0"
  }
}
